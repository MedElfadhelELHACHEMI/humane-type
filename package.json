{
  "name": "humanetype",
  "version": "1.1.4",
  "description": "React component library for adding natural, humanized variations to text",
  "author": "Med Elfadhel EL HACHEMI",
  "license": "CC-BY-4.0",
  "type": "module",
  "main": "dist/humane.umd.js",
  "module": "dist/humane.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/humane.es.js",
      "require": "./dist/humane.umd.js"
    },
    "./style.css": "./dist/humane.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsc -b && vite build",
    "build:app": "tsc -b && vite build --config vite.config.app.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0", 
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^3.8.0"
  },
  "keywords": [
    "react",
    "typography",
    "humanized",
    "text",
    "handwriting",
    "design",
    "ui",
    "components",
    "indesign",
    "humane-type"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MedElfadhelELHACHEMI/humane-type.git"
  },
  "bugs": {
    "url": "https://github.com/MedElfadhelELHACHEMI/humane-type/issues"
  },
  "homepage": "https://github.com/MedElfadhelELHACHEMI/humane-type#readme",
  "publishConfig": {
    "access": "public"
  }
}
